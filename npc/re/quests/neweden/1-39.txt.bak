moc_para01,36,38,3	script	Mission [1 - 39]#Tuto	4_BOARD3,{

// Questname,questid,minlv,maxlv,monster,amount,bexp,jexp,edencoin
function	script	edenquest	{
	.@size = getarraysize(getarg(0));
	copyarray .array,getarg(0),.@size;
	.questname$ = .array[0];
	.questid	= .array[1];
	.minlv		= .array[2];
	.maxlv		= .array[3];
	.monster	= .array[4];
	.amount		= .array[5];
	.bexp		= .array[6];
	.jexp		= .array[7];
	.edencoin	= .array[8];
	
end;
}